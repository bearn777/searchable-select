{
    "name": "bearn777/searchable-select",
    "description": "A searchable select field for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "select",
        "searchable"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bearn777\\SearchableSelect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bearn777\\SearchableSelect\\FieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
